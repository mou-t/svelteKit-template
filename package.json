{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"aspida:build": "aspida -c aspida.config.cjs",
		"aspida:convert": "node scripts/convertTypeOnlyImport.cjs 'src/aspida/$api.ts'",
		"api:build": "run-s aspida:build aspida:convert",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@aspida/fetch": "^1.7.1",
		"@sveltejs/adapter-vercel": "^1.0.0-next.32",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"aspida": "^1.7.1",
		"autoprefixer": "^10.4.0",
		"cssnano": "^5.0.13",
		"daisyui": "^1.16.6",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.6",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"type": "module"
}
